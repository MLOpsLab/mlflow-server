# MLflow Tracking URI
MLFLOW_TRACKING_URI=http://mlflow-server:5000

# Artifact location (can be local or cloud-based)
MLFLOW_ARTIFACT_ROOT=/mlflow/mlruns

# Backend store (SQLite by default, or Postgres if you want later)
MLFLOW_BACKEND_STORE_URI=sqlite:///mlflow.db

# Use this when you're deploying to a remote cloud storage (like AWS S3)
# MLFLOW_ARTIFACT_ROOT=s3://your-bucket-name/mlflow

# Set log level for MLflow (useful for debugging)
MLFLOW_LOG_LEVEL=INFO
